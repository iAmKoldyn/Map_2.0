{
  "name": "travel-service-backend",
  "version": "1.0.0",
  "description": "Backend service for travel application using Express, tRPC, and Prisma",
  "main": "dist/app.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/app.js",
    "dev": "ts-node-dev --respawn --transpile-only src/app.ts",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "seed": "ts-node prisma/seed.ts",
    "test-client": "ts-node src/test-client.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "@trpc/client": "^10.45.2",
    "@trpc/server": "^10.45.2",
    "@types/jsonwebtoken": "^9.0.9",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.16",
    "prisma": "^5.10.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
